<header>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    
    <% if(!user){ %>
      <h1>
        <a href="/login">
          <button class="btn btn-outline-primary">Login</button>
        </a>
     </h1>
     
     <h1> 
        <a href="/register">
        <button class="btn btn-outline-success">Register</button>
        </a>
     </h1>
    

    <% } else{ %>  
      <h1 class="h6">
        <form method="POST" action="/logout">
          <span class="font-weight-light, text-white bg-dark">Logged in as: <%= user.email%></span>
          <button class="btn btn-danger">Logout</button>
        </form>
      </h1>
   <% } %>  

  </nav>
</header>